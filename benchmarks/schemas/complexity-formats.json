    "patternProperties": {
      "^email[0-9]+$": { "type": "string", "format": "email" },
      "^uuid[0-9]+$": { "type": "string", "format": "uuid" },
      "^date[0-9]+$": { "type": "string", "format": "date-time" },
      "^uri[0-9]+$": { "type": "string", "format": "uri" },
      "^ip[0-9]+$": { "type": "string", "format": "ipv4" }
