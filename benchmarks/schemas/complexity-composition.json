          },
          {
            "allOf": [
              {
                "type": "object",
                "properties": { "prop3": { "type": "boolean" } }
              },
              {
                "allOf": [
                  {
                    "type": "object",
                    "properties": { "prop4": { "type": "string" } }
                  },
                  {
                    "allOf": [
                      {
                        "type": "object",
                        "properties": { "prop5": { "type": "number" } }
                      },
                      {
                        "allOf": [
                          {
                            "type": "object",
                            "properties": { "prop6": { "type": "boolean" } }
